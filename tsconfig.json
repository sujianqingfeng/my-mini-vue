{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": ["DOM", "ES6"],
    "noImplicitAny": false,

    "types": ["jest"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true // 允许set-> for of 迭代
  }
}
